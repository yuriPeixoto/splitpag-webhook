{
    "name": "yuripeixoto/splitpag-webhook",
    "description": "SWebhook system for SplitPag API integration",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "stan": "phpstan analyse src tests"
    },
    "authors": [
        {
            "name": "Yuri Peixoto",
            "email": "yuripeixoto@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "slim/slim": "^4.11",
        "slim/psr7": "^1.6",
        "php-di/php-di": "^7.0",
        "monolog/monolog": "^3.3",
        "vlucas/phpdotenv": "^5.5",
        "guzzlehttp/guzzle": "^7.5",
        "respect/validation": "^2.2",
        "ramsey/uuid": "^4.7",
        "symfony/cache": "^6.0",
        "doctrine/orm": "^2.11"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "squizlabs/php_codesniffer": "^3.7",
        "phpstan/phpstan": "^1.10"
    }
}
